{
  "name": "shardeum-website",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc",
    "format:check": "npx prettier -c \"**/*.{js,jsx,json,ts,tsx}\"   ",
    "format": "npx prettier -w \"**/*.{js,jsx,json,ts,tsx}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.7",
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "airtable": "^0.11.3",
    "axios": "^0.26.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "framer-motion": "^6.2.8",
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-fast-marquee": "^1.3.1",
    "swiper": "^8.1.3",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@types/airtable": "^0.10.1",
    "@types/axios": "^0.14.0",
    "@types/node": "17.0.14",
    "@types/react": "17.0.39",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "husky": "^7.0.0",
    "lint-staged": ">=10",
    "prettier": "2.6.1",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,json,ts,tsx}": [
      "eslint",
      "prettier --write"
    ]
  }
}
